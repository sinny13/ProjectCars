<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.mr.mapper.ReserveMapper">

  <insert id="reserveInsert" parameterType="reserveDTO">
     insert into reservation(revName, revJumin1,revJumin2,revTel,revAddr,revDate1,revDate2,dateCnt,totalPrice)
     values(#{revName}, #{revJumin1},#{revJumin2},#{revTel},#{revAddr},#{revDate1},#{revDate2},#{dateCnt},#{totalPrice})
  </insert>



  <select id="reserveList" resultType="reserveDTO">
  	select * from reservation
  </select>
  
  
  
  
  
  
  

</mapper>